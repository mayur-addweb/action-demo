{% if show_title %}
    <h2 class="block-title">My Electronic Materials Archive</h2>
{% endif %}
<div class="views-element-container form-group">
    <div class="view view-my-events view-id-my_events">
        {% if in_person is not empty %}
            <h3 class="accent-left group-title">In-Person</h3>
            <div class="group group-in_person">
            {% for event in in_person %}
                {% if event.electronic_materials %}
                    <div class="views-row" data-begin-date="{{ event.event_begin_date }}" data-end-date="{{ event.event_end_date }}" data-expiry-date="{{ event.expiry_date }}" data-product-id="{{ event.product_id }}" data-event-code="{{ event.event_code }}" data-event-year="{{ event.event_year }}" data-uid="{{ uid }}" data-name-id="{{ am_net_name_id }}" data-registration-date="{{ event.registration_date }}" data-registration-status-code="{{ event.registration_status_code }}" data-division="{{ event.division }}">
                        <div class="views-field views-field-field-dates-times">
                            <div class="field-content">
                                {{ event.expiry_month }}<br>{{ event.expiry_day }}/{{ event.expiry_year }}
                            </div>
                        </div>
                        <div class="views-field views-field-title">
                            <strong class="field-content">{{ event.title }}</strong>
                        </div>
                        <div class="views-field views-field-body">
                            <div class="field-content">
                                {{ event.summary|raw }}
                            </div>
                        </div>
                        <div class="views-field views-field-field-event-link views-field-field-electronic-material">
                            <div class="field-content">
                                <div class="panel panel-default">
                                    <div class="panel-heading" role="tab" id="header-in-person">
                                        <span class="panel-title">
                                            <a role="button" data-toggle="collapse" data-parent="#accordion" href="#collapse-{{ event['product_id'] }}" aria-expanded="false" aria-controls="collapse-{{ event['product_id'] }}">
                                                Electronic Materials
                                            </a>
                                        </span>
                                    </div>
                                    <div id="collapse-{{ event['product_id'] }}" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="header-{{ event['product_id'] }}">
                                        <div class="panel-body">
                                            {% for electronic_material in event.electronic_materials %}
                                                {{ electronic_material }}
                                            {% endfor %}
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                {% endif %}
            {% endfor %}
            </div>
        {% endif %}
        {% if online is not empty %}
            <h3 class="accent-left group-title">Online</h3>
            <div class="group group-online">
                {% for event in online %}
                    {% if event.electronic_materials %}
                        <div class="views-row" data-begin-date="{{ event.event_begin_date }}" data-end-date="{{ event.event_end_date }}" data-expiry-date="{{ event.expiry_date }}" data-product-id="{{ event.product_id }}"  data-event-code="{{ event.event_code }}" data-event-year="{{ event.event_year }}" data-uid="{{ uid }}" data-name-id="{{ am_net_name_id }}" data-registration-date="{{ event.registration_date }}" data-registration-status-code="{{ event.registration_status_code }}" data-division="{{ event.division }}">
                            <div class="views-field views-field-field-dates-times">
                                <div class="field-content">
                                    {{ event.expiry_month }}<br>{{ event.expiry_day }}/{{ event.expiry_year }}
                                </div>
                            </div>
                            <div class="views-field views-field-title">
                                <strong class="field-content">{{ event.title }}</strong>
                            </div>
                            <div class="views-field views-field-body">
                                <div class="field-content">
                                    {{ event.summary|raw }}
                                </div>
                            </div>
                            <div class="views-field views-field-field-event-link views-field-field-electronic-material">
                                <div class="field-content">
                                    <div class="panel panel-default">
                                        <div class="panel-heading" role="tab" id="header-online">
                                            <span class="panel-title">
                                                <a role="button" data-toggle="collapse" data-parent="#accordion" href="#collapse-{{ event['product_id'] }}" aria-expanded="false" aria-controls="collapse-{{ event['product_id'] }}">
                                                    Electronic Materials
                                                </a>
                                            </span>
                                        </div>
                                        <div id="collapse-{{ event['product_id'] }}" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="header-{{ event['product_id'] }}">
                                            <div class="panel-body">
                                                {% for electronic_material in event.electronic_materials %}
                                                    {{ electronic_material }}
                                                {% endfor %}
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    {% endif %}
                {% endfor %}
            </div>
        {% endif %}
        {% if on_demand is not empty %}
            <h3 class="accent-left group-title">On-Demand</h3>
            <div class="group group-on_demand">
                {% for course in on_demand %}
                    {% if course.electronic_materials %}
                        <div class="views-row" data-product-id="{{ course.product_id }}"  data-product-code="{{ course.product_code }}" data-uid="{{ uid }}" data-name-id="{{ am_net_name_id }}" data-order-date="{{ course.order_date }}" data-division="{{ course.division }}">
                            <div class="views-field views-field-field-dates-times">
                                <span class="label">EXPIRES ON</span>
                                <div class="field-content">
                                    {{ course.expiration_date_month }}&nbsp;{{ course.expiration_date_day }}<br>{{ course.expiration_date_year }}
                                </div>
                            </div>
                            <div class="views-field views-field-title">
                                <strong class="field-content">{{ course.title }}</strong>
                            </div>
                            <div class="views-field views-field-body">
                                <div class="field-content">
                                    {{ course.summary|raw }}
                                </div>
                            </div>
                            <div class="views-field views-field-field-course-link views-field-field-electronic-material">
                                <div class="field-content">
                                    <div class="panel panel-default">
                                        <div class="panel-heading" role="tab" id="header-on-demand">
                                            <span class="panel-title">
                                                <a role="button" data-toggle="collapse" data-parent="#accordion" href="#collapse-{{ course['product_id'] }}" aria-expanded="false" aria-controls="collapse-{{ course['product_id'] }}">
                                                    Electronic Materials
                                                </a>
                                            </span>
                                        </div>
                                        <div id="collapse-{{ course['product_id'] }}" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="header-{{ course['product_id'] }}">
                                            <div class="panel-body">
                                                {% for electronic_material in course.electronic_materials %}
                                                    {{ electronic_material }}
                                                {% endfor %}
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    {% endif %}
                {% endfor %}
            </div>
        {% endif %}
    </div>
</div>
