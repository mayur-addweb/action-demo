    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php",
            "scripts/composer/UtProjectScriptHandler.php"
        ]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
            "cp config/sso/config/AuthnRequest.php ./vendor/simplesamlphp/saml2/src/SAML2/AuthnRequest.php",
            "cp config/sso/config/authsources.php ./vendor/simplesamlphp/simplesamlphp/config/authsources.php",
            "cp config/sso/config/config.php ./vendor/simplesamlphp/simplesamlphp/config/config.php",
            "cp config/sso/metadata/saml20-idp-remote.php ./vendor/simplesamlphp/simplesamlphp/metadata/saml20-idp-remote.php",
            "mkdir -p ./vendor/simplesamlphp/simplesamlphp/cert",
            "cp config/sso/cert/saml.crt ./vendor/simplesamlphp/simplesamlphp/cert/saml.crt || echo 'saml.crt is missing, skipping this step'",
            "cp config/sso/cert/saml.pem ./vendor/simplesamlphp/simplesamlphp/cert/saml.pem || echo 'saml.pem is missing, skipping this step'",
            "patch web/.htaccess < ./patches/htaccess-saml.patch || echo 'Failed to patch file - maybe it was already patched before?'"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
            "cp config/sso/config/authsources.php ./vendor/simplesamlphp/simplesamlphp/config/authsources.php",
            "cp config/sso/config/config.php ./vendor/simplesamlphp/simplesamlphp/config/config.php",
            "cp config/sso/metadata/saml20-idp-remote.php ./vendor/simplesamlphp/simplesamlphp/metadata/saml20-idp-remote.php",
            "mkdir -p ./vendor/simplesamlphp/simplesamlphp/cert",
            "cp config/sso/cert/saml.crt ./vendor/simplesamlphp/simplesamlphp/cert/saml.crt || echo 'saml.crt is missing, skipping this step'",
            "cp config/sso/cert/saml.pem ./vendor/simplesamlphp/simplesamlphp/cert/saml.pem || echo 'saml.pem is missing, skipping this step'",
            "patch web/.htaccess < ./patches/htaccess-saml.patch || echo 'Failed to patch file - maybe it was already patched before?'"
        ],
        "post-create-project-cmd": [
            "UtDrupalProject\\composer\\UtProjectScriptHandler::postCreateProject"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"]
        },
        "patches": {
            "drupal/commerce_paypal": {
                "Allow new PayFlow payment methods to be altered before save": "https://patch-diff.githubusercontent.com/raw/drupalcommerce/commerce_paypal/pull/8.diff"
            },
            "drupal/core": {
                "Error element - https://www.drupal.org/project/drupal/issues/2027059": "./patches/core/2027059-75.patch",
                "Multiple usages of FieldPluginBase::getEntity do not check for NULL, leading to WSOD": "https://www.drupal.org/files/issues/2020-05-15/3007424-104.patch"
            },
            "drupal/ctools": {
                "Correct entity field block issue due to subform state issue": "https://www.drupal.org/files/issues/2800951-2-subform_state.patch"
            },
            "drupal/disqus": {
                "Unable to Preview https://www.drupal.org/node/2718557": "https://www.drupal.org/files/issues/2019-09-23/Cannot-Preview-A-New-Node-2718557-9.patch"
            },
            "drupal/commerce_google_tag_manager": {
                "Fixing Price on the GTA push event": "./patches/commerce_google_tag_manager/commerce_google_tag_manager.patch"
            },
            "drupal/licensing": {
                "Missing schema": "https://www.drupal.org/files/issues/2941176-1.patch",
                "Licensing Condition issue": "./patches/licensing/licensing.patch"
            },
            "drupal/sitewide_alert": {
                "Exclude js file from aggregation": "./patches/sitewide_alert_module_js_aggr_exclude.patch"
            },
            "simplesamlphp/simplesamlphp": {
                "Deprecated function: assert(): Calling assert() with a string argument is deprecated": "./patches/simplesamlphp/simplesamlphp-lib-SimpleSAML.patch"
            }
        }
    }
